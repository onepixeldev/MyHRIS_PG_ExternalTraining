	<subDataset name="verBySvc" uuid="ab4b8442-0a71-44f7-b58d-d9534c218846">
		<property name="com.jaspersoft.studio.data.sql.tables" value=""/>
		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="MyHRIS_PG"/>
		<parameter name="verById" class="java.lang.String"/>
		<queryString language="SQL">
			<![CDATA[SELECT 
CASE 
WHEN (APM_DESC IS NOT NULL AND SS_DESC_SHORT IS NOT NULL OR (SS_DESC_SHORT IS NULL OR SS_DESC_SHORT = '')) THEN APM_DESC
WHEN ((APM_DESC IS NULL OR APM_DESC = '') AND SS_DESC_SHORT IS NOT NULL) THEN SS_DESC_SHORT
WHEN ((APM_DESC IS NULL OR APM_DESC = '') AND (SS_DESC_SHORT IS NULL OR SS_DESC_SHORT = '')) THEN ''
END AS "VER_SVC"
FROM (
    SELECT INITCAP(APM_DESC) APM_DESC, SM1.SM_STAFF_ID SID1,
    (
    	SELECT INITCAP(SS_DESC_SHORT) SS_DESC_SHORT
		FROM STAFF_MAIN SM2
        LEFT JOIN SERVICE_SCHEME ON SM_JOB_CODE = SS_SERVICE_CODE
		WHERE SM_STAFF_ID = $P{verById} 
    )
		FROM STAFF_MAIN SM1
        LEFT JOIN ADMIN_POST_MAIN ON SM_ADMIN_JOBCODE = APM_CODE
		WHERE SM_STAFF_ID = $P{verById} 
) AS TB1]]>
		</queryString>
		<field name="VER_SVC" class="java.lang.String">
			<property name="com.jaspersoft.studio.field.label" value="VER_SVC"/>
		</field>
	</subDataset>
