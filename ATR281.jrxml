	<queryString language="SQL">
		<![CDATA[SELECT TH_REF_ID AS "TH_REF_ID" ,
TH_TRAINING_TITLE AS "TH_TRAINING_TITLE",
TH_ORGANIZER_NAME AS "TH_ORGANIZER_NAME",
