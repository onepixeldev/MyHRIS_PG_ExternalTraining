to_char(STH_VERIFY_DATE,'dd/mm/yyyy') AS "STH_VERIFY_DATE",
STH_APPROVE_REASON AS "STH_APPROVE_REASON",
STH_APPROVE_BY AS "STH_APPROVE_BY",
