		<parameter name="TRAINING_YEAR" class="java.lang.String"/>
		<queryString language="SQL">
			<![CDATA[SELECT DISTINCT 
TH_REF_ID AS "TH_REF_ID",
TH_TRAINING_TITLE AS "TH_TRAINING_TITLE",
TO_CHAR(TH_DATE_FROM,'dd/mm/yyyy')||' hingga '||TO_CHAR(TH_DATE_TO,'dd/mm/yyyy') AS "TARIKH",
TH_TRAINING_VENUE AS "TH_TRAINING_VENUE", 
TH_TRAINING_STATE AS "TH_TRAINING_STATE", SM_STATE_DESC AS "SM_STATE_DESC",
TH_TRAINING_COUNTRY AS "TH_TRAINING_COUNTRY", CM_COUNTRY_DESC AS "CM_COUNTRY_DESC",
TH_ORGANIZER_NAME AS "TH_ORGANIZER_NAME",
TOH_ORG_DESC AS "TOH_ORG_DESC"
FROM TRAINING_HEAD
JOIN STAFF_TRAINING_HEAD ON TH_REF_ID = STH_TRAINING_REFID
LEFT JOIN STAFF_MAIN ON STH_STAFF_ID = SM_STAFF_ID
LEFT JOIN DEPARTMENT_MAIN ON SM_DEPT_CODE = DM_DEPT_CODE
LEFT JOIN STATE_MAIN ON TH_TRAINING_STATE = SM_STATE_CODE
LEFT JOIN COUNTRY_MAIN ON TH_TRAINING_COUNTRY = CM_COUNTRY_CODE
LEFT JOIN TRAINING_ORGANIZER_HEAD ON TOH_ORG_CODE = TH_ORGANIZER_NAME
WHERE TH_INTERNAL_EXTERNAL = 'EXTERNAL_AGENCY'
AND TH_STATUS = 'APPROVE'
AND STH_STATUS = 'REJECT' 
AND STH_APPROVE_BY IS NULL
AND ($P{DEPARTMENT} IS NULL OR ($P{DEPARTMENT} IS NOT NULL AND SM_DEPT_CODE = $P{DEPARTMENT}))
AND ($P{TRAINING_MM} IS NULL OR ($P{TRAINING_MM} IS NOT NULL AND TO_CHAR(TH_DATE_FROM,'mm') = $P{TRAINING_MM}))
AND TO_CHAR(TH_DATE_FROM,'yyyy') = $P{TRAINING_YEAR}]]>
		</queryString>
